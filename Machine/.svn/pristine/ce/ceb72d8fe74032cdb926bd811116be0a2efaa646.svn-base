	//用户管理
	function u_userinfo(){
		var t = $("#tabsdiv").tabs("exists", "用户管理");
		var content = "<iframe scrolling='no' frameborder='0'  src='ym_user/u_userinfo.jsp' style='width:100%;height:100%;'></iframe>";
		if (t == true) {
			$("#tabsdiv").tabs("select", "用户管理");
			/* alert("你需要的信息已打开！"); */
		} else {
			$("#tabsdiv").tabs("add", {
				title : "用户管理",
				content : content,
				/* href:"/testhref.jsp", */
				closable : true
			});
		}
	}
	
